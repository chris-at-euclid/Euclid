    <script>
    	$(document).ready( function() {
				$('.home-slides').cycle({ pager: '#home-slides-pager', prev: '#home-slides-left', next: '#home-slides-right', speed:   1000, pause: 1, timeout: 8000  });
				
				$('.home-slides-pager-wrapper .next-prev').mouseover( function() {  $(this).addClass('hover'); });
				$('.home-slides-pager-wrapper .next-prev').mouseout( function() {  $(this).removeClass('hover'); });
				$('#home-slides-pager, .home-slides-pager-wrapper .next-prev').click( function() { $('.home-slides').cycle('pause'); });
			});
    </script>









